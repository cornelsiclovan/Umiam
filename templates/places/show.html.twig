{% extends 'base.html.twig' %}

{% block title %}

    {{ place.name }}
{% endblock %}

{% block body %}
    {{ parent() }}
    <div class="row">
        <div class="col-sm-12">
            <h1 class="">Manage place: {{ place.name }}</h1>
            <table class="table table-striped">
                <tr>
                    <td scope="col">#</td>
                    <th scope="col">Name</th>
                    <th scope="col">City</th>
                    <th scope="col">Address</th>
                    <th scope="col">Action</th>
                </tr>




            </table>
        </div>
    </div>
{% endblock %}